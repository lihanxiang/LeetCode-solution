//遍历，将每一行旋转和对调
class Solution {
    public int[][] flipAndInvertImage(int[][] A) {
        int[][] result = new int[A.length][];
        for (int i = 0; i < A.length; i++) {
            result[i] = new int[A[i].length];
            for (int j = 0; j < A[i].length; j++) {
                result[i][j] = A[i][A[i].length - j - 1];
                if (result[i][j] == 1){
                    result[i][j] = 0;
                } else {
                    result[i][j] = 1;
                }
            }
        }
        return result;
    }
}