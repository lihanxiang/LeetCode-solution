//将数组排序完了之后选取单数元素相加就行，排序算法越快越好，否则超时
import java.util.Arrays;
class Solution {
    public int arrayPairSum(int[] nums) {
        int sum = 0;
        for (int i = 0; i < nums.length; i++) {
            if (nums[i] < 10000 || nums[i] > -10000){
                Arrays.sort(nums);
            }
        }
        for (int i = 0; i < nums.length; i+= 2) {
            sum += nums[i];
        }
        return sum;
    }
}