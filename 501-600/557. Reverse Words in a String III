class Solution {
    public String reverseWords(String s) {
        String result = "";
        String temp = "";
        char[] c = s.toCharArray();
        for (int i = 0; i <= s.length(); i++) {
            if (i == c.length || c[i] == ' '){
                result += (temp + " ");
                temp = "";
            } else {
                //把字符一个个地顶到后面去
                temp = c[i] + temp;
            }
        }
        return result.trim();
    }
}