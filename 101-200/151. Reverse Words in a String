//提交时的测试用例中可能有多个空格，所以要用正则表达式匹配一至多个空格
public class Solution {
    public String reverseWords(String s) {
        String[] strings = s.trim().split("\\s+");
        String result = "";
        for (int i = strings.length - 1; i > 0; i--) {
            result += strings[i] + " ";
        }
        result += strings[0];
        return result;
    }
}