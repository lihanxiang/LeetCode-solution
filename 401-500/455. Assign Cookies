class Solution {
    public int findContentChildren(int[] g, int[] s) {
        if (g.length == 0 || s.length == 0){
            return 0;
        }
        Arrays.sort(g);
        Arrays.sort(s);
        int i = 0, j = 0;
        for (; i < g.length && j < s.length; j++){
            if (g[i] <= s[j]){
                i++;
            }
        }
        return i;
    }
}