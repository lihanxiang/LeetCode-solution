class Solution {
    public boolean judgeCircle(String moves) {
        int x = 0, y = 0;
        char[] c = moves.toCharArray();
        for (int i = 0; i < c.length; i++) {
            if (c[i] == 'L'){
                x--;
            } else if (c[i] == 'R'){
                x++;
            } else if (c[i] == 'U'){
                y--;
            } else {
                y++;
            }
        }
        return x == 0 && y == 0;
    }
}