class Solution {
    public int mySqrt(int x) {
        int root = 1;
        if (x == 0){
            return 0;
        }
        while (root <= x / root){
            if (root <= x / root && (root + 1) > x / (root + 1)){
                break;
            }
            root++;
        }
        return root;
    }
}