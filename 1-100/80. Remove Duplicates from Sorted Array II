class Solution {
    public int removeDuplicates(int[] nums) {
        if (nums.length == 0){
            return 0;
        }
        int cur = 0;
        for (int i = 0; i < nums.length; i++){
            if (i < 2 || nums[i] > nums[cur - 2]){
                nums[cur++] = nums[i];
            }
        }
        return cur;
    }
}