//只检测左半边符号，只推入右半边符号
class Solution {
    public boolean isValid(String s) {
        char[] c = s.toCharArray();
        Stack<Character> stack = new Stack<>();
        for (int i = 0; i < c.length; i++) {
            if (c[i] == '(') {
                stack.push(')');
            } else if (c[i] == '[') {
                stack.push(']');
            } else if (c[i] == '{') {
                stack.push('}');
            }
			//如果一开始就是 ([{，则直接返回 false, 然后每次循环依次出栈
			else if (stack.size() == 0 || stack.pop() != c[i]){
                return false;
            }
        }
        return stack.size() == 0;
    }
}