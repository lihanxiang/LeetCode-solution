class Solution {
    public List<String> generateParenthesis(int n) {
        List<String> list = new ArrayList<>();
        DFS("", list, n, n);
        return list;
    }
    
    public void DFS(String s, List<String> list, int left, int right){
        if (left > right){
            return;
        }
        if (left > 0){
            DFS(s + "(", list, left - 1, right);
        }
        if (right > 0){
            DFS(s + ")", list, left, right - 1);
        }
        if (left == 0 && right == 0){
            list.add(s);
        }
    }
}