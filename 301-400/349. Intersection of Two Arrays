//既然要去重，就直接用现有的数据结构 set 来做，不考虑多次添加，因为 set 会去重
class Solution {
    public int[] intersection(int[] nums1, int[] nums2) {
        Set<Integer> set = new HashSet<>();
        for (int i : nums1) {
            for (int j : nums2) {
                if (i == j) {
                    set.add(i);
                }
            }
        }
        int[] result = new int[set.size()];
        int i = 0;
        for (Integer num :
                set) {
            result[i++] = num;
        }
        return result;
    }
}