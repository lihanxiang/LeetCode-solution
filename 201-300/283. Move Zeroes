//直接在原数组上做改动
class Solution {
    public void moveZeroes(int[] nums) {
        int zero = 0;
        for (int i = 0, j = 0; i < nums.length; i++) {
            if (nums[i] != 0){
                nums[j] = nums[i];
                j++;
            }
            else {
                zero++;
            }
        }
        for (int i = 0; i < zero; i++) {
            nums[nums.length - i - 1] = 0;
        }
    }
}